{
  "name": "LockIn: Study Tracker ",
  "description": "",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["storage", "activeTab", "scripting", "tabs", "windows"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
    "action": {
    "default_title": "LockIn: Study Tracker"
  },
     "icons": {
        "16": "public/LockinLogo.png",
        "32": "public/LockinLogo.png",
        "48": "public/LockinLogo.png",
        "128": "public/LockinLogo.png"
    },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_start",
      "all_frames": true,
      "match_about_blank": true
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["webgazer.js"],
      "matches": ["<all_urls>"]
    }
  ]
}